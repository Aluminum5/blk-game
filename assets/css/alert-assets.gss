/**
 * Embedded assets used by the alerts styles.
 *
 * @author benvanik@google.com (Ben Vanik)
 */

@defmixin blkAlertBackground() {
  /** alert-middle.png */
  background-image: url(data:image/png;base64,);
}

@defmixin blkAlertHeaderBackground() {
  /** alert-top.png */
  background-position: 0 -19px;
  background-image: url(data:image/png;base64,);
}

@defmixin blkAlertButtonBackground() {
  /** alert-button.png */
  background-image: url(data:image/png;base64,);
}

@defmixin blkAlertButtonDefaultBackground() {
  /** alert-button-default.png */
  background-image: url(data:image/png;base64,);
}
